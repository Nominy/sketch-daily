{
  "name": "sketch.intelrug.ru",
  "version": "1.0.0",
  "description": "Sketch",
  "author": "Svyatoslav Fyodorov",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build:main": "tsc -p tsconfig.main.json",
    "build:dist": "nuxt-ts build && yarn build:main",
    "build:pack": "electron-builder",
    "build": "yarn build:dist && yarn build:pack",
    "dev": "yarn build:main && concurrently -k -n main,renderer -c blue,green \"cross-env NODE_ENV=development electron .\" \"nuxt-ts dev\"",
    "lint:script": "eslint --ext .ts,.js,.vue --ignore-path .gitignore .",
    "format:script": "prettier --write ./**/*.{js,ts,vue}",
    "format:styl": "stylus-supremacy format ./**/*.styl --replace --options ./stylus-supremacy.json",
    "typegen": "graphql-codegen",
    "posttypegen": "prettier --write ./graphql/**/*.ts",
    "predeploy": "yarn build",
    "deploy": "node ./scripts/deploy.js",
    "test": "jest",
    "mc": "node ./scripts/make-component.js"
  },
  "main": "dist/main",
  "build": {
    "appId": "ru.intelrug.sketch",
    "files": [
      {
        "from": "dist",
        "to": "dist",
        "filter": [
          "main/**/*",
          "renderer/**/*"
        ]
      },
      {
        "from": ".",
        "filter": [
          "package.json"
        ]
      }
    ]
  },
  "dependencies": {
    "@nuxt/typescript-runtime": "^0.4.0",
    "@nuxtjs/apollo": "^4.0.0-rc19",
    "@nuxtjs/dotenv": "^1.4.0",
    "@nuxtjs/svg-sprite": "^0.4.10",
    "apollo-link-error": "^1.1.13",
    "core-js": "2",
    "express": "^4.17.1",
    "express-useragent": "^1.0.13",
    "get-port": "^5.1.1",
    "graphql-tag": "^2.10.3",
    "nuxt": "^2.0.0",
    "nuxt-i18n": "^6.9.1",
    "nuxt-property-decorator": "^2.5.1",
    "universal-cookie": "^4.0.3",
    "vue": "^2.6.11",
    "vue-router": "^3.1.6",
    "vuex": "^3.1.3",
    "vuex-class": "^0.3.2",
    "vuex-class-component": "^2.2.1",
    "wait-on": "^5.0.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.13.2",
    "@graphql-codegen/typescript": "^1.13.2",
    "@graphql-codegen/typescript-graphql-files-modules": "^1.13.2",
    "@graphql-codegen/typescript-operations": "^1.13.2",
    "@nuxt/types": "^0.7.4",
    "@nuxt/typescript-build": "^0.6.0",
    "@nuxtjs/eslint-config-typescript": "^1.0.0",
    "@nuxtjs/eslint-module": "^1.0.0",
    "@nuxtjs/router": "^1.5.0",
    "@nuxtjs/style-resources": "^1.0.0",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/express": "^4.17.6",
    "@types/express-useragent": "^1.0.0",
    "@types/node": "12",
    "@types/wait-on": "^4.0.0",
    "@vue/test-utils": "^1.0.0-beta.27",
    "babel-eslint": "^10.0.1",
    "concurrently": "^5.2.0",
    "cross-env": "^7.0.2",
    "electron": "^9.0.0",
    "electron-builder": "^22.6.1",
    "electron-devtools-installer": "^3.0.0",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.1.2",
    "gh-pages": "^2.2.0",
    "prettier": "^1.19.1",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "stylus-supremacy": "^2.14.3",
    "ts-loader": "^7.0.4",
    "typescript": "^3.9.3"
  }
}
